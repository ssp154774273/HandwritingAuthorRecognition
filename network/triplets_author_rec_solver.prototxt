net: "network/author_rec_train_test.prototxt"
test_iter: 100
test_interval: 100
base_lr: 0.0001
momentum: 0.9
weight_decay: 0.0005
# The learning rate policy
lr_policy: "step"
gamma: 0.1
# every stepsize iters lr will be mult with gamma
stepsize: 1000
# Display every 
display: 1
max_iter: 2500000
# snapshot every
snapshot: 10
snapshot_prefix: "network/snapTriplet/snap_triplet_author_rec"
solver_mode: GPU
